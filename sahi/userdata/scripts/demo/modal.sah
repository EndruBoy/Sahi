if (_isOpera()){
	// Opera does not support Modal Dialogs
} else if (_isIE()) {
	_wait(1000);
	_click(_button("Set 1"));
	_wait(1000);
	_focusWindow();
	_click(_checkbox(0));
	_click(_button("Close"));
	_assertEqual("123", _lastAlert());
} else {
	_click(_button("Set 1"));
	_focusWindow();
	_popup("me modal")._click(_checkbox(0));
	_popup("me modal")._click(_button("Close"));
	_assertEqual("123", _lastAlert());
}